"*":
  "atom-jshint":
    hintOnModify: false
  "autocomplete-plus":
    confirmCompletion: "tab"
  core:
    disabledPackages: [
      "language-clojure"
      "language-go"
      "language-java"
      "language-mustache"
      "language-perl"
      "language-toml"
      "linter-phpcs"
      "linter-eslint"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".sass-cache"
      ".DS_Store"
      "composer.lock"
    ]
    projectHome: "~/Code"
    themes: [
      "atom-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  dash:
    grammars:
      ActionScript: [
        "actionscript"
      ]
      CSS: [
        "css"
        "bootstrap"
      ]
      HTML: [
        "html"
        "underscore"
        "backbone"
        "jquery"
        "javascript"
        "css"
      ]
      JavaScript: [
        "nodejs"
        "underscore"
        "backbone"
        "grunt"
        "express"
        "require"
        "jquery"
        "javascript"
      ]
      PHP: [
        "php"
        "laravel"
      ]
      SQL: [
        "mysql"
      ]
      SVG: [
        "svg"
      ]
      Sass: [
        "compass"
        "sass"
        "css"
      ]
      "Shell Scripts": [
        "bash"
        "manpages"
      ]
  docblockr: {}
  editor:
    autoIndentOnPaste: false
    fontSize: 12
    invisibles: {}
    nonWordCharacters: "./\\\\()\\\"'-:,.;<>~!@#$%^&*|+=[]{}`~?"
    showIndentGuide: true
    tabLength: 4
  "exception-reporting":
    userId: "bf3ab42e-1c26-e9b9-141a-bbcd1e367d22"
  "gist-it": {}
  "git-diff":
    showIconsInEditorGutter: true
  jshint: {}
  linter:
    lintOnChange: false
    showErrorPanel: false
    showGutters: false
    statusBar: "Show error if the cursor is in range"
    subtleLinterErrors: [
      "jshint"
      "php"
      "phpcs"
      "phpmd"
    ]
  "linter-jshint": {}
  "linter-php": {}
  "linter-phpcs": {}
  metrics:
    userId: "d4f04106661c7c892c48e7ea675a18896355e99a"
  react: {}
  "release-notes":
    viewedVersion: "0.92.0"
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    removeTrailingWhitespace: false
".arduino.source":
  editor:
    nonWordCharacters: "./\\\\()\\\"'-:,.;<>~!@#$%^&*|+=[]{}`~?"
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
".coffee.jsx.source":
  editor:
    nonWordCharacters: "./\\\\()\\\"':,.;<>~!@#$%^&*|+=[]{}`~?"
".js.jsx.source":
  editor:
    nonWordCharacters: "./\\\\()\\\"':,.;<>~!@#$%^&*|+=[]{}`~?"
".js.source":
  editor:
    nonWordCharacters: "./\\\\()\\\"':,.;<>~!@#$%^&*|+=[]{}`~?"
